<!-- Content Start -->
<div id="main">
  <!-- Slider Start-->
  <div class="slider-wrapper theme-default">
    <div id="nivoslider" class="nivoSlider">
      <%= link_to (image_tag('slider/visme.jpg', :title => '#htmlcaption')), '#' %>
      <%= link_to (image_tag('slider/academyofideas.jpg', :title => '#htmlcaption2')), '#' %>
      <%= link_to (image_tag('slider/foundandlostdogs.jpg', :title => '#htmlcaption3')), '#' %>
    </div>
    <div id="htmlcaption" class="nivo-html-caption">
      <h2>Terminal cancer patient visitor planner</h2>
    </div>
    <div id="htmlcaption2" class="nivo-html-caption">
      <h2>Innovative, effective and fun way to get the best ideas and information from your employees and
        co-workers.</h2>
    </div>
    <div id="htmlcaption3" class="nivo-html-caption">
      <h2>Found Lost Dogs.</h2>
    </div>
  </div>
  <div class="clearfix"></div>
  <!-- Slider End-->
  <!-- Slogan Start-->
  <div class="slogan bottom-pad-small">
    <div class="container">
      <div class="slogan-content">
        <div class="col-lg-9 col-md-9">
          <h2 class="slogan-title">We have hot openings in ROR and Android!</h2>
        </div>
        <div class="col-lg-3 col-md-3">
          <div class="get-started">
            <%= link_to 'Apply Now', contact_path, :class => 'btn btn-special btn-color pull-right' %>
          </div>
        </div>
        <div class="clearfix"></div>
      </div>
    </div>
  </div>
</div>
<!-- Slogan End-->
<!-- Main Content start-->

<div class="main-content">
  <div class="container">
    <div class="row">
      <div class="col-lg-4 col-md-4 col-sm-4">
        <div class="content-box big ch-item bottom-pad-small">
          <div class="ch-info-wrap">
            <div class="ch-info">
              <div class="ch-info-front ch-img-1"><i class="fa fa-building-o"></i></div>
              <div class="ch-info-back">
                <i class="fa fa-building-o"></i>
              </div>
            </div>
          </div>
          <div class="content-box-info">
            <h3>Web Development</h3>

            <p>
              Moorem Technologies is specialist in Web Development using Ruby On Rails Technology Stack.
            </p>
          </div>
        </div>
      </div>
      <div class="col-lg-4 col-md-4 col-sm-4">
        <div class="content-box big ch-item bottom-pad-small">
          <div class="ch-info-wrap">
            <div class="ch-info">
              <div class="ch-info-front ch-img-1"><i class="fa fa-building-o"></i></div>
              <div class="ch-info-back">
                <i class="fa fa-building-o"></i>
              </div>
            </div>
          </div>
          <div class="content-box-info">
            <h3>Mobile Development</h3>
            <p>
              We follow the latest trends in mobile development and develop productive apps and business solutions for
              iPhone, iPad and Android.
            </p>
          </div>
        </div>
      </div>
      <div class="col-lg-4 col-md-4 col-sm-4">
        <div class="content-box big ch-item">
          <div class="ch-info-wrap">
            <div class="ch-info">
              <div class="ch-info-front ch-img-1"><i class="fa fa-building-o"></i></div>
              <div class="ch-info-back">
                <i class="fa fa-building-o"></i>
              </div>
            </div>
          </div>
          <div class="content-box-info">
            <h3>Technologies We Support</h3>

            <p>
              Ruby On Rails, Backbone, Angularjs, CoffeeScript, HTML5, Responsive Designs, Objective-C, Android SDK,
              Java, Rhodes Framework.
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

